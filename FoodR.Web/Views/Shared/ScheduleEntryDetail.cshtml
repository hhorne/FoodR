@model ScheduleEntryDetailsViewModel
<li class="entry">
    <div class="timespan">
        <div>@Model.From.ToString("h tt")</div>
        <div>@Model.To.ToString("h tt")</div>
    </div>
    <div class="location">
        <h5>@Model.Location.Name</h5>
        <p>@Model.Location.StreetAddress</p>
    </div>
    @if (false)
    {
        <div class="entry-controls">
            <button class="btn btn-info btn-sm">Edit</button>
            <button class="btn btn-danger btn-sm">Cancel This</button>
        </div>
    }
    <div class="clearfix"></div>
</li>

